<mvc:View controllerName="testTree.testTree.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:ui="sap.ui.table">
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="Attributes pulled from FSM shell app">
                <content>
                    <ui:Table
                    	id="equipTable"
                    	selectionMode="None"
						rows="{eqModel>/equipments}"
						ariaLabelledBy="title">
						<ui:extension>
							<OverflowToolbar style="Clear">
								<Title id="title" text="Liste des Ã©quipements"/>
							</OverflowToolbar>
						</ui:extension>
						<ui:columns>
							<ui:Column >
								<Label text="Description" />
								<ui:template>
									<Text text="{Name}" wrapping="false" />
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="Utilisation" />
								<ui:template>
									<Text text="{Type}"/>
								</ui:template>
							</ui:Column>
						</ui:columns>
					</ui:Table>
                </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>